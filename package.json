{
  "name": "tifis",
  "version": "0.1.0",
  "author": "sezzh",
  "description": "Tifis theme inspired by some delightful and minimal themes.",
  "scripts": {
    "build-stylus-prod": "stylus -c -u nib ./src/stylus/styles.styl -o ./assets/css/screen.css",
    "build-stylus-dev": "stylus -u nib -w ./src/stylus/styles.styl -o ./assets/css/screen.css",
    "check-syntax-stylus": "stylint ./src/stylus --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "nib": "^1.1.2",
    "sf-grid": "^1.0.2",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5",
    "webpack": "^2.2.1"
  }
}
